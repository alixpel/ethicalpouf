CAAA,SAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,aAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAlBA,EAmBA,KAnBA,CAmBA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OArBA;AAsBA;AA1BA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"\">\n    <h2>Ajouter un artisan</h2>\n    <form>\n        <label for=\"\">name</label>\n        <input type=\"text\" v-model=\"name\"/><br>\n\n        <label for=\"presentation\">présentation</label>\n        <input type=\"text\" v-model=\"presentation\"/><br>\n\n        <label for=\"thumbnail\">thumbnail</label>\n        <input type=\"text\" v-model=\"thumbnail\">\n\n        <label for=\"photo\">photo</label>\n        <input type=\"text\" v-model=\"photo\">\n\n\n        <!-- <label for=\"thumbnail\">Portrait de l'artisan : </label>\n        <picture-input type=\"file\" v-model=\"thumbnail\" @change=\"onChange()\" width=\"500\" height=\"500\" margin=\"16\" accept=\"image/jpeg,image/png\">\n        </picture-input><br> -->\n\n        <!-- photo -->\n\n        <label for=\"origine\">origine</label>\n        <input type=\"text\" v-model=\"origine\"/><br>\n\n        <label for=\"firstPage\">première page</label>\n        <input type=\"text\" v-model=\"firstPage\"/><br>\n\n        <label for=\"artisanDuMois\">artisan du mois</label>\n        <input type=\"text\" v-model=\"artisanDuMois\"/><br>\n\n        <button type=\"button\" @click=\"postArtisan\">Envoyer</button>\n    </form>\n <!--la réponse du formulaire ci-dessous si ça a été validé:  -->\n    <p v-if=\"success\">L'artisan' a bien été enregistré</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"NouvelArtisan\",\n  data() {\n    return {\n       name: \"\",\n       presentation: \"\",\n       thumbnail: \"\",\n       photo: \"\",\n       origine: \"\",\n       firstPage: false,\n       artisanDuMois: false,\n       success: false,\n    }\n  },\n  methods: {\n    postArtisan() {\n      const axios = require('axios');\n  //dans axios.post{2 arguments 1/ le chemin URL, 2/ le body, c'est les infos qu'on envoie à l'API et c'est toujours un objet}\n      axios.post(`http://localhost:3000/artisans/nouveau`, {\n          name: this.name,\n          presentation: this.presentation,\n          thumbnail: this.thumbnail,\n          photo: this.photo,\n          origine: this.origine,\n          firstPage: this.firstPage,\n          artisanDuMois: this.artisanDuMois,\n      })\n      .then(() => {\n        this.name = \"\";\n        this.presentation = \"\";\n        this.thumbnail = \"\";\n        this.photo = \"\";\n        this.origine = \"\";\n        this.firstPage = \"\";\n        this.artisanDuMois = \"\";\n        this.success = true;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}