       <label for=\"\">nom</label>\n        <input type=\"text\" v-model=\"name\"/>\n      </div>\n      <div class=\"field\">\n        <label for=\"presentation\">présentation</label>\n        <textarea v-model=\"presentation\" rows=\"15\" cols=\"55\"></textarea>\n      </div>\n\n      <!-- photo PARTIE ALIX-->\n\n      <div class=\"field\">\n        <label for=\"origine\">origine</label>\n        <input type=\"text\" v-model=\"origine\"/>\n      </div>\n      <div class=\"field\">\n        <label for=\"firstPage\">première page</label>\n        <input type=\"text\" v-model=\"firstPage\"/>\n      </div>\n      <div class=\"field\">\n        <label for=\"artisanDuMois\">artisan du mois</label>\n        <input type=\"text\" v-model=\"artisanDuMois\"/>\n      </div>\n      <button type=\"button\" @click=\"putArtisan()\">Envoyer</button>\n    </form>\n     <!-- </div> -->\n <!--la réponse du formulaire ci-dessous si ça a été validé:  -->\n    <p v-if=\"success\">L'artisan a bien été modifié</p>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ModifierArtisan\",\n  data() {\n    return {\n    artisans: \"\",\n        id: \"\",\n       name: \"\",\n       presentation: \"\",\n       thumbnail: \"\",\n       photo: \"\",\n       origine: \"\",\n       firstPage: false,\n       artisanDuMois: false,\n       success: false,\n    }\n  },\n\n    mounted() {\n     const axios = require(\"axios\");\n     axios.get('http://localhost:3000/artisans')\n     .then(response => (this.artisans = response.data))\n   },\n\n  methods: {\n    putArtisan() {\n        const axios = require('axios');\n        axios.put(`http://localhost:3000/changeArtisan/${this.id}`, {\n          name: this.name,\n          presentation: this.presentation,\n          thumbnail: this.thumbnail,\n          photo: this.photo,\n          origine: this.origine,\n          firstPage: this.firstPage,\n          artisanDuMois: this.artisanDuMois,\n      })\n      .then(() => {\n        this.name = \"\";\n        this.presentation = \"\";\n        this.thumbnail = \"\";\n        this.photo = \"\";\n        this.origine = \"\";\n        this.firstPage = \"\";\n        this.artisanDuMois = \"\";\n        this.success = true;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n    }\n  }\n}\n</script>\n\n<style  lang=\"css\" scoped>\n\n  .artisan-form {\n    background-color: rgb(240, 240, 240);\n    margin: auto;\n    padding: 1rem;\n    width: 95vw;\n  }\n\n  form {\n    margin: auto;\n    margin-top: 2rem;\n    width: 65%;\n  }\n\n  label {\n    text-align: left;\n    width: 18%;\n  }\n\n  input, textarea {\n    border: 0px;\n    border-radius: 1rem;\n    width: 50%;\n  }\n\n  button {\n    background-color: #006845;\n    border: none;\n    border-radius: 1rem;\n    color: white;\n    font-size: 0.9rem;\n    font-weight: 500;\n    margin: 2rem 0rem;\n    padding: 0.2rem 1rem 0.3rem 1rem;\n    text-align: center;\n  }\n\n\n@media only screen and (max-device-width: 1024px) {\n\n  form {\n    width: 100%;\n  }\n\n  label {\n    font-size: 1.2rem;\n    width: 24%;\n  }\n\n  input, textarea {\n    font-size: 1.2rem;\n  }\n\n  button {\n    font-size: 1.2rem;\n  }\n\n}\n\n@media only screen and (min-device-width: 375px) and (max-device-width: 667px) {\n\n\n}\n</style>\n"]}]}