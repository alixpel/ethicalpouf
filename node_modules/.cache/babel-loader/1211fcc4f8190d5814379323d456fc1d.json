th: 64vw;\n}\n\n.search-bar {\n  margin-right: 1rem;\n}\n\n.pop-up-searchbar-container {\n  align-items: center;\n  display: flex;\n  justify-content: center;\n}\n\n.pop-up-searchbar {\n  background-color: white;\n  padding: 1rem;\n  position: absolute;\n}\n\n\n/* TITLES */\n\n\n/* TEXT */\n\n.covid-banner p {\n  margin-bottom: 0rem;\n  text-align: center;\n}\n\n// LINKS\n\n.menu a {\n  color: #006845;\n  font-size: 1.2rem;\n  font-weight: 600;\n}\n\n.menu a:hover {\n  color: #11a331;\n  font-weight: 900;\n\ttext-decoration: none;\n  transition: all 0.5s ease-out;\n  }\n\n.menu a:not(:hover) {\n  transition: all 0.5s ease-out;\n}\n\n\n/* NAVS & LISTS */\n\n.menu ul {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  margin: 0rem;\n  padding-left: 0rem;\n  width: 100%;\n}\n\n.menu li {\n  list-style-type: none;\n}\n\n// .menu li:before {\n//   content: \"\";\n//   position: absolute;\n//   width: 100%;\n//   height: 1px;\n//   bottom: 0;\n//   left:\t0;\n//   background-color: #1a535c;\n//   visibility: hidden;\n//   transform: scaleX(0);\n//   transition: all .3s ease-in-out;\n// }\n//\n// .menu li:hover:before {\n//   visibility: visible;\n//   transform: scaleX(1);\n// }\n\n\n/* INPUTS */\n\n.pop-up-searchbar input {\n  border: 0rem;\n  border-bottom: 1mm ridge rgb(220, 220, 220, .3);\n}\n\n/* BUTTONS */\n\n.covid-banner button {\n  background-color: rgba(255, 255, 255, 0);\n  border: none;\n  margin-left: 2rem;\n}\n\n.close-button,\n.searchbar-button,\n.research-button {\n  background-color: rgba(255, 255, 255, 0);\n  border: none;\n  margin-left: 2rem;\n}\n\n\n/* IMG */\n\n.logo {\n  max-width: 10vw;\n}\n\n\n\n\n/*================= MEDIA QUERIES =================*/\n // and (min-device-width: 768px)\n@media only screen and (max-device-width: 1024px) {\n\n  .desktop-header {\n    display: none;\n  }\n\n  .smartphone-header {\n    align-items: flex-start;\n    display: flex;\n    flex-direction: column;\n  }\n\n  .burger-and-title {\n